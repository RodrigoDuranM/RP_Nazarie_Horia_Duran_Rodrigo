<launch>
    <node name="info_user" pkg="game_control" type="info_user.py" output="screen">
    </node>

    <node name="game_node" pkg="game_control" type="game_node.py" output="screen">
        <param name="user_name" value="Rodrigo" />
        <param name="screen_param" value="phase1" />
        <param name="change_player_color" value="1" />
    </node>

    <node name="control_node" pkg="game_control" type="control_node.py" output="screen" />

    <node name="control_node_pygame" pkg="game_control" type="control_node_pygame.py" output="screen" />

    <node name="result_node" pkg="game_control" type="result_node.py" output="screen" />
</launch>
